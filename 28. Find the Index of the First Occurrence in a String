class Solution {
    public int strStr(String haystack, String needle) {
        int num1=haystack.length();
        int num2=needle.length();
        int ptr=0;
        for(int i=0,j=num2;j<=num1;i++,j++){
            if(haystack.substring(i,j).equals(needle)){
                return i;
            }
            
        }
        return -1;
    }
}
